/** search form css */
#epl_ps_tab_1 > form {
    /* margin-bottom: 200px; */
    position: relative;
    float: left;
    margin: 0 auto;
    /* text-align: center; */
    overflow: visible;
    display: table;
    width: 100%;
}
#epl_ps_tab_1 > form .more-options {
    bottom: -30px;
    display: inline-block;
    height: 30px;
    right: 0;
    padding: 0 10px;
    position: absolute;
    transform: translateX(-5.85%);
    width: auto;
}
 form .more-options .fa.fa-chevron-down, form .more-options .fa.fa-chevron-up{
    color: #ffffff;
}
.hsq-checkbox label {
    cursor: pointer;
    font-weight: normal;
    line-height: 45px;
    margin: 0;
    padding-left: 30px;
    position: relative;
}
.hsq-checkbox input {
    display: none;
}
.hsq-checkbox label span::before {
    color: #bfa249;
    display: block;
    font-size: 0.8em;
    left: 3px;
    line-height: 20px;
    position: absolute;
    top: -12px;
    transform: scale(0);
    transition: all 0.3s ease 0s;
}

.hsq-checkbox label span::before{
    -moz-osx-font-smoothing: grayscale;
    display: inline-block;
    font-family: "fontawesome";
    font-style: normal;
    font-weight: normal;
    line-height: 1;
    position: relative;
    top: -15px;
    left: 5px;
    content: "\f00c";
}
.hsq-checkbox label span {
    background: #ffffff none repeat scroll 0 0;
    border: 1px solid #bfa249;
    height: 20px;
    left: 1px;
    position: absolute;
    top: 1px;
    width: 20px;
}
.hsq-checkbox label > input:checked + span::before {
    transform: scale(1);
}
.hsq-checkbox.check-box-container > label {
    color: #333333;
}
